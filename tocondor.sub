executable = subash.sh
optimizer=adam
problem=XXZ
initl=6
args=$(Item) $(optimizer) $(problem) $(initl)
output = ../jobs/$(problem)_$(Item).out
error = ../jobs/$(problem)_$(Item).err
log = ../jobs/$(problem)_$(Item).log
request_memory = 12 GB
request_cpus = 6
requirements= Has_avx == true
+flavour="medium"

queue from seq 0.0 1.0 5.0|
