executable = subash.sh
optimizer=adam
problem=XXZ
args=$(Item) $(optimizer) $(problem)
output = ../jobs/$(problem)_J$(Item).out
error = ../jobs/$(problem)_J$(Item).err
log = ../jobs/$(problem)_J$(Item).log
request_memory = 6 GB
request_cpus = 2
requirements= Has_avx2 == true
+flavour="short"

queue from seq 0 0.25 2|