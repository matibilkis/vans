executable = subash.sh
optimizer=adam
problem=XXZ
initl=4
args=$(Item) $(optimizer) $(problem) $(initl)
output = ../jobs/$(problem)_J$(Item).out
error = ../jobs/$(problem)_$(Item).err
log = ../jobs/$(problem)_J$(Item).log
request_memory = 12 GB
request_cpus = 6
requirements= Has_avx == true
+flavour="medium"

queue from seq 0.4 0.2 1.8|
